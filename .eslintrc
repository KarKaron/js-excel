{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "eol-last": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off",
    "one-var": "off",
    "quotes": ["error", "double", { "allowTemplateLiterals": true }]
  },
  "extends": ["eslint:recommended", "google"]
}